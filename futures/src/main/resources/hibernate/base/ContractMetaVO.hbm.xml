<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ltc.base.vo.ContractMetaVO" table="BASE_CONTRACT_META">
        <id	name="symbol" type="string">
            <column name="SYMBOL" />
            <generator class="assigned" />
        </id>
        <property name="ctpSymbol" type="string">
            <column name="CTP_SYMBOL" />
        </property>
        <property name="description" type="string">
            <column name="DESC" length="8"/>
        </property>
        <property name="leverage" type="float">
            <column name="LEVERAGE"/>
        </property>
        <property name="pointValue" type="int">
            <column name="POINT_VAL"/>
        </property>
        <property name="atr" type="float">
            <column name="ATR"/>
        </property>
        <property name="atrUpdateDate" type="date">
            <column name="ATR_UPD_DATE"/>
        </property>
        <property name="priority" type="int">
            <column name="PRIORITY"/>
        </property>
        <many-to-one name="group" class="com.ltc.base.vo.GroupVO" fetch="select">
            <column name="GROUP_ID" not-null="false" />
        </many-to-one>
    </class>
</hibernate-mapping>