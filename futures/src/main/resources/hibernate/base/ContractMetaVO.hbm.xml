<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ltc.base.vo.ContractMetaVO" table="CONTRACT_META" catalog="BASE">
        <id name="id" type="java.lang.Long">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="symbol" type="string">
            <column name="SYMBOL" length="5" not-null="true" unique="true" />
        </property>
        <property name="description" type="string">
            <column name="DESC" length="8"/>
        </property>
        <property name="handValue" type="int">
            <column name="HAND_VALUE"/>
        </property>
        <property name="leverage" type="float">
            <column name="LEVERAGE"/>
        </property>
        <property name="pointValue" type="int">
            <column name="POINT_VAL"/>
        </property>
        <property name="atr" type="float">
            <column name="ATR"/>
        </property>
    </class>
</hibernate-mapping>